{
    "name": "@simonverhoeven/cdk-app",
    "version": "0.1.0",
    "bin": {
        "packages": "src/index.ts"
    },
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "watch": "tsc -w -p tsconfig.build.json",
        "synth": "npx cdk synth",
        "test": "jest",
        "cdk": "cdk",
        "clean:light": "rm -rf ./dist && rm -rf tsconfig.build.tsbuildinfo",
        "clean:full": "yarn run clean:light && rm -rf ./node_modules && rm -rf ./cdk.out",
        "typecheck": "tsc --noEmit",
        "precommit": "yarn run typecheck"
    },
    "devDependencies": {
        "@types/node": "10.17.27",
        "ts-node": "^8.1.0",
        "typescript": "~3.9.7"
    },
    "dependencies": {
        "@aws-cdk/assert": "1.75.0",
        "@aws-cdk/aws-apigateway": "1.75.0",
        "@aws-cdk/aws-codepipeline": "1.75.0",
        "@aws-cdk/aws-codepipeline-actions": "1.75.0",
        "@aws-cdk/aws-cognito": "1.75.0",
        "@aws-cdk/aws-iam": "1.75.0",
        "@aws-cdk/aws-lambda": "1.75.0",
        "@aws-cdk/aws-logs": "1.75.0",
        "@aws-cdk/aws-sns": "1.75.0",
        "@aws-cdk/aws-sns-subscriptions": "1.75.0",
        "@aws-cdk/aws-sqs": "1.75.0",
        "@aws-cdk/aws-ssm": "1.75.0",
        "@aws-cdk/core": "1.75.0",
        "@aws-cdk/pipelines": "1.75.0",
        "@aws-solutions-constructs/core": "1.75.0",
        "@simonverhoeven/lambda-a": "^2.0.0",
        "@simonverhoeven/lambda-b": "^2.0.0",
        "@simonverhoeven/lambda-gq-resolver": "^2.0.0",
        "@simonverhoeven/lambda-user-confirmed": "^2.0.0",
        "aws-cdk": "1.75.0",
        "json-beautify": "^1.1.1"
    }
}
